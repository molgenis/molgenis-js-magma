{
  "name": "@molgenis/magma-script",
  "version": "0.1.1",
  "description": "MagmaScript evaluator in JavaScript",
  "main": "dist/MagmaScript.js",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "unpkg": "dist/MagmaScript.min.js",
  "scripts": {
    "unit": "gulp",
    "lint": "gulp lint",
    "debug": "gulp test-browser",
    "watch": "gulp watch",
    "build": "gulp build",
    "coverage": "gulp coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/molgenis/molgenis-js-magma.git"
  },
  "keywords": [
    "MagmaScript"
  ],
  "author": "Fleur Kelpin <f.kelpin@umcg.nl>",
  "license": "LGPL-3.0+",
  "bugs": {
    "url": "https://github.com/molgenis/molgenis-js-magma/issues"
  },
  "homepage": "https://github.com/molgenis/molgenis-js-magma",
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.13.0",
    "babel-preset-latest": "6.14.0",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "del": "2.2.2",
    "glob": "7.0.6",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-filter": "4.0.0",
    "gulp-istanbul": "1.1.1",
    "gulp-livereload": "3.8.1",
    "gulp-load-plugins": "1.2.4",
    "gulp-mocha": "3.0.1",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "2.0.0",
    "isparta": "4.0.0",
    "json-loader": "0.5.4",
    "mocha": "3.0.2",
    "semantic-release": "^19.0.3",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "webpack": "1.13.2",
    "webpack-stream": "3.2.0"
  },
  "babelBoilerplateOptions": {
    "entryFileName": "MagmaScript.js",
    "mainVarName": "MagmaScript"
  },
  "dependencies": {
    "mathjs": "^4.3.0",
    "moment": "^2.22.1"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  }
}
